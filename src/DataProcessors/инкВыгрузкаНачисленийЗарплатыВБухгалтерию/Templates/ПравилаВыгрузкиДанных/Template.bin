        КонецЕсли;
        
        докВыч.ВычетыНаДетей.Очистить();
        
        Для каждого элМассива Из мСтВыч Цикл				

            // Получим даду С и ПО:
            Если Не ЗначениеЗаполнено(элМассива.ДействуетС) Тогда
                элДатаС = НачалоГода(ТекущаяДата());		
            Иначе
                Если элМассива.ДействуетС &lt; НачалоГода(ТекущаяДата()) Тогда
                    элДатаС = НачалоГода(ТекущаяДата());
                Иначе
                    элДатаС = элМассива.ДействуетС;
                КонецЕсли;                    
            КонецЕсли;          
            Если Не ЗначениеЗаполнено(элМассива.ДействуетПо) Тогда
                элДатаПо = КонецГода(ТекущаяДата());		
            Иначе
                Если элМассива.ДействуетПо &lt; НачалоГода(ТекущаяДата()) Тогда
