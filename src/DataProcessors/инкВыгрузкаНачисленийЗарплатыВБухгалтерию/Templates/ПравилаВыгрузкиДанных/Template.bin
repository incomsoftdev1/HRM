                ссКодВычетаДети = Справочники.ВидыВычетовНДФЛ.НайтиПоКоду(элМассива.ВычетКод);	
            КонецЕсли;				 
                        
            Если ЗначениеЗаполнено(ссКодВычета) Тогда
                Если (элМассива.ВычетКод = "104") ИЛИ (элМассива.ВычетКод = "105") Тогда
                    докВыч.ИзменитьЛичныйВычет = Истина;
                    докВыч.КодВычетаЛичный = ссКодВычета;
                Иначе 
                    докВыч.ИзменитьВычетыНаДетей = Истина;
                    стрВычетыНаДетей = докВыч.ВычетыНаДетей.Добавить();					
                    стрВычетыНаДетей.КодВычета = ссКодВычета;
                    стрВычетыНаДетей.ДействуетДо = элДатаПо;	
                КонецЕсли; 
            КонецЕсли;
                        
        КонецЦикла;
        
        Попытка
            
            Если (докВыч.ВычетыНаДетей.Количество() &gt; 0) ИЛИ
