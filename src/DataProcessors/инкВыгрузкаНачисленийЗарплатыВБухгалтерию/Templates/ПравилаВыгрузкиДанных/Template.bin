                Иначе
                    элДатаС = элМассива.ДействуетС;
                КонецЕсли;                    

            КонецЕсли; 
                        
            ссКодВычета = Справочники.ВидыВычетовНДФЛ.ПустаяСсылка(); 
            
            Если элМассива.ВычетКод = "126/114" Тогда
                ссКодВычета = Справочники.ВидыВычетовНДФЛ.Код108;
