        КонецЦикла;
        
        Если ЗначениеЗаполнено(ЗаявлениеДокумент) Тогда
            докВыч = ЗаявлениеДокумент.ПолучитьОбъект();
        Иначе
            докВыч = Документы.ЗаявлениеНаПредоставлениеСтандартныхВычетовПоНДФЛ.СоздатьДокумент();
            докВыч.Дата = ТекущаяДата();				
            докВыч.Месяц = НачалоГода(ТекущаяДата());
            докВыч.Организация = ссГоловнаяОрганизация;
